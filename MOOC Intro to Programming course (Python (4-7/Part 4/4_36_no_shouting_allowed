def no_shouting(word_list: list):
    new_list = []
    for item in word_list:
        if item.isupper() == False:
            new_list.append(item)
        else:
            continue
    return new_list



my_list = ["ABC", "def", "UPPER", "ANOTHERUPPER", "lower", "another lower", "Capitalized"]
pruned_list = no_shouting(my_list)
print(pruned_list)