def even_numbers(ulist: list):
    even = []
    uneven = []
    for number in ulist:
        if number % 2 ==0:
            even.append(number)
        else:
            uneven.append(number)
    result = even
    return result


my_list = [1, 2, 3, 4, 5]
new_list = even_numbers(my_list)
print("original", my_list)
print("new", new_list)